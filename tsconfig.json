{
  "compilerOptions": {
    "jsx": "react-jsx",
    "outDir": "dist",
    "module": "ESNext",
    "target": "ESNext",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "allowJs": true,
    "strict": true
  },
  "include": [
    "server", 
    "resume-parser",
  "global.d.ts",
   "components/**/*",
   "lib/**/*",
   "server/**/*",
   "resume-parser/**/*",
   "page.tsx"]
}
